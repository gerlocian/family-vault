{
  "name": "family-vault",
  "version": "0.0.1",
  "description": "Online application for tracking movies and books owned by the family.",
  "main": "index.js",
  "scripts": {
    "pretest": "npm run test:security",
    "start": "babel-node server/server.js",
    "start:dev": "npm run start -s -- --development",
    "test": "npm-run-all --parallel test:security test:unit test:lint",
    "test:lint": "esw --color server/**/*.js",
    "test:lint:watch": "npm run test:lint -- --watch",
    "test:security": "nsp check",
    "test:unit": "mocha --reporter progress --compilers js:babel-core/register server/**/*.integration.js server/**/*.unit.js",
    "test:unit:watch": "npm run test:unit -- --watch",
    "test:watch": "npm-run-all --parallel test:security test:unit:watch test:lint:watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gerlocian/family-vault.git"
  },
  "keywords": [
    "movie",
    "book",
    "tracking",
    "family"
  ],
  "author": "Patrick Ortiz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gerlocian/family-vault/issues"
  },
  "homepage": "https://github.com/gerlocian/family-vault#readme",
  "dependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-polyfill": "^6.22.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-register": "^6.22.0",
    "body-parser": "^1.16.1",
    "bootstrap": "^3.3.7",
    "express": "^4.14.1",
    "jquery": "^3.2.1",
    "mongodb": "^2.2.22",
    "morgan": "^1.8.0",
    "npm-run-all": "^4.0.1",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-router-dom": "^4.1.1",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "babel-loader": "^6.4.1",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "css-loader": "^0.26.4",
    "eslint": "^3.15.0",
    "eslint-watch": "^2.1.14",
    "eventsource-polyfill": "^0.9.6",
    "file-loader": "^0.11.2",
    "localtunnel": "^1.8.2",
    "lodash": "^4.17.4",
    "mocha": "^3.2.0",
    "node-sass": "^4.5.3",
    "nsp": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "testdouble": "^1.11.1",
    "testdouble-chai": "^0.5.0",
    "url-loader": "^0.5.9",
    "webpack": "^2.6.1",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-hot-middleware": "^2.18.0"
  }
}
